   "source": [
    "# Minimum number of reviews required to qualify for a recommendation, as discussed above\n",
    "review_threshold = 85 \n",
    "\n",
    "# Filter the movie_stats table to get top movies which meet the threshold\n",
    "movie_stats_filtered = movie_stats[movie_stats['Total_reviews']>review_threshold].sort_values(by='bayesian_avg', ascending=False)\n",
    "\n",
    "# Filtering movie_ratings table for the same top movies\n",
    "movie_ratings_topn = movie_ratings[movie_ratings['title'].isin(movie_stats_filtered['title'])]\n",
    "\n",
    "# Create a user-movie matrix to represent user preferences\n",
    "user_matrix = movie_ratings_topn.pivot_table(index='userId', columns='title', values='rating')\n",
    "\n",
    "user_matrix\n"
   ]
  },
