    "target_user = 1 # Select a user between 1-610\n",
    "user_similarity_threshold = 0.4 # Minimum similarity score\n",
    "n_top_correlations = 10 # Number of top similar users to pick\n",
    "max_recos = 15\n",
    "\n",
    "MAE_error = 1.1 # Maximum MAE error for user-based recommendation\n",
    "min_predictions = 5 # Minimum predictions required for user-based recommendation\n",
    "\n",
    "evaluation_dict = {\n",
    "    'userId':[],\n",
    "    'num_movies_evaluated':[],\n",
    "    'MAE':[],\n",
    "    'RMSE':[]\n",
    "}\n",
    "\n",
    "if target_user in (user_matrix.index):\n",
    "\n",
    "    user_avg_rating = user_mean_ratings[target_user]\n",
    "\n",
    "    # Based on Pearson similarities, find top n similar users\n",
    "    user_corr = user_similarity.loc[target_user].drop(labels=target_user).sort_values(ascending=False)\n",
    "    top_n_similar_users = user_corr[user_corr > user_similarity_threshold].head(n_top_correlations)\n",
    "\n",
    "    if len(top_n_similar_users) > 0: # This removes the user which didn't have any similar users\n",
    "\n",
    "        # Table for movies watched by the target user\n",
    "        target_userid_watched = norm_user_matrix[norm_user_matrix.index == target_user].dropna(axis=1, how='all')\n",
    "\n",
    "        # Table for movies watched by correlated users\n",
    "        movies_similar_user = norm_user_matrix[norm_user_matrix.index.isin(top_n_similar_users.index)].dropna(axis=1, how='all')\n",
    "\n",
    "        # Scoring movies\n",
    "        movie_score = {}\n",
    "        for mov in movies_similar_user:\n",
    "            total = 0\n",
    "            count = 0\n",
    "            for n in movies_similar_user.index:\n",
    "                if pd.notna(movies_similar_user[mov][n]):\n",
    "                    score = movies_similar_user[mov][n] * top_n_similar_users[n]\n",
    "                    total += score\n",
    "                    count += 1\n",
    "\n",
    "            movie_score[mov] = total/count\n",
    "\n",
    "        # Creating a scoring table\n",
    "        score = pd.DataFrame(movie_score.items(), columns=['title', 'movie_score']).sort_values(by='movie_score', ascending=False)\n",
    "        score['predicted_rating'] = score['movie_score'] + user_avg_rating\n",
    "        score.loc[score['predicted_rating'] > 5, 'predicted_rating'] = 5\n",
    "\n",
    "        # Calculating errors\n",
    "        error = score[score['title'].isin(target_userid_watched.columns)]\n",
    "        ratings_by_target_user = movie_ratings[movie_ratings['userId'] == target_user]\n",
    "\n",
    "        error = error.merge(ratings_by_target_user, how='left', on='title').drop(['userId', 'movieId', 'timestamp', 'genres'], axis=1)\n",
    "\n",
    "        error['MAE'] = abs(error['rating'] - error['predicted_rating'])\n",
    "        error['squared_error'] = (error['rating'] - error['predicted_rating']) ** 2\n",
    "\n",
    "        avg_MAE = error['MAE'].mean()\n",
    "        avg_RMSE = np.sqrt(error['squared_error'].mean())\n",
    "\n",
    "        num_pred = len(error)\n",
    "\n",
    "        evaluation_dict['userId'].append(target_user)\n",
    "        evaluation_dict['num_movies_evaluated'].append(num_pred)\n",
    "        evaluation_dict['MAE'].append(avg_MAE)\n",
    "        evaluation_dict['RMSE'].append(avg_RMSE)\n",
    "    \n",
    "    else: # Filling in the dictionary for users with no correlated users\n",
    "        evaluation_dict['userId'].append(target_user)\n",
    "        evaluation_dict['num_movies_evaluated'].append(0)\n",
    "        evaluation_dict['MAE'].append(None)\n",
    "        evaluation_dict['RMSE'].append(None)\n",
    "\n",
    "\n",
    "else: # Filling in the dictionary for users with no correlated users\n",
    "    evaluation_dict['userId'].append(target_user)\n",
    "    evaluation_dict['num_movies_evaluated'].append(0)\n",
    "    evaluation_dict['MAE'].append(None)\n",
    "    evaluation_dict['RMSE'].append(None)\n",
    "\n",
    "evaluation_matrix = pd.DataFrame(evaluation_dict)\n",
    "evaluation_matrix.set_index('userId', inplace=True)\n",
    "\n",
    "# Check if user meets the conditions\n",
    "user_num_predictions = evaluation_matrix.loc[target_user]['num_movies_evaluated']\n",
    "user_MAE = evaluation_matrix.loc[target_user]['MAE']\n",
    "user_RMSE = evaluation_matrix.loc[target_user]['RMSE']\n",
