   "source": [
    "# Calculate the mean ratings for all users\n",
    "user_mean_ratings = user_matrix.mean(axis=1) \n",
    "\n",
    "# Subtract the mean from all their ratings to create a normalized matrix\n",
    "norm_user_matrix = user_matrix.subtract(user_mean_ratings, axis='rows')\n",
    "\n",
    "norm_user_matrix\n"
   ]
