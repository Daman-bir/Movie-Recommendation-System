    "top_genres_user = {\n",
    "    'userId':[],\n",
    "    'genre_count':[],\n",
    "    'Top_genres':[]\n",
    "}\n",
    "\n",
    "user_list = sorted(movie_ratings['userId'].unique().tolist())\n",
    "\n",
    "for n in user_list:\n",
    "    picked_user = n\n",
    "    genre_frequency = {}\n",
    "\n",
    "    for movie_genres in movie_ratings[movie_ratings['userId'] == picked_user]['genres']:\n",
    "        for genre in movie_genres:\n",
    "            genre_frequency.setdefault(genre, 0)\n",
    "            genre_frequency[genre] += 1\n",
    "\n",
    "    genre_freq = pd.DataFrame([genre_frequency]).T.reset_index()\n",
    "    genre_freq.columns = ['genre', 'count']\n",
    "\n",
    "    # sorting genres for the user\n",
    "    sorted_genres = sorted(genre_frequency.items(), key=lambda x: x[1], reverse=True)\n",
    "\n",
    "    # Pick top 5 genres\n",
    "    top_5_genres = dict(sorted_genres[:5])\n",
    "    top_5_genre_keys = list(top_5_genres.keys())\n",
    "\n",
    "    top_genres_user['userId'].append(picked_user)\n",
    "    top_genres_user['genre_count'].append(sorted_genres)\n",
    "    top_genres_user['Top_genres'].append(top_5_genre_keys)\n",
    "\n",
    "\n",
    "user_genre_list = pd.DataFrame(top_genres_user)\n",
    "user_genre_list = user_genre_list.set_index('userId')\n",
    "user_genre_list"
   ]
