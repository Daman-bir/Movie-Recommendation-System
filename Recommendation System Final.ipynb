    "# Score all the movies using weighted average:\n",
    "\n",
    "movie_score = {}\n",
    "\n",
    "for mov in movies_similar_user:\n",
    "    total = 0\n",
    "    count = 0\n",
    "    for n in movies_similar_user.index:\n",
    "        if pd.isna(movies_similar_user[mov][n])==False:\n",
    "            score = movies_similar_user[mov][n] * top_n_similar_users[n]\n",
    "            total += score\n",
    "            count +=1\n",
    "    \n",
    "    movie_score[mov] = total/count\n",
    "        \n",
    "movie_score"
   ]
